# Your Google Calendar ID (found in calendar settings)
# Create a new calendar specifically for this project!
GOOGLE_CALENDAR_ID=

# Your Epitech intranet session cookie
USER_COOKIE=

# Epitech API base URL (don't change)
API_BASE_URL=https://intra.epitech.eu

# Local server port
PORT=8888

# Path to Google OAuth credentials file
# Get this from https://console.cloud.google.com/apis/credentials
# Create new OAuth 2.0 Client ID (Web application type)
# Set redirect URI to http://localhost
CRED_PATH=credentials.json

# Path where Google OAuth tokens will be stored
TOKEN_PATH=token.json
